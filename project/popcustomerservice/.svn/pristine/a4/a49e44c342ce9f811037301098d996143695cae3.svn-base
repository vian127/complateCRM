<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="keywords" content="CRM" />
    <meta name="description" content="CRM" />
    <title>客户详情</title>
    <!-- css -->
    <link rel="stylesheet" type="text/css" th:href="@{/css/common/base.css}" />
    <link rel="stylesheet" type="text/css" th:href="@{/css/lib/bootstrap/bootstrap.min.css}" />
    <link rel="stylesheet" type="text/css" th:href="@{/css/common/font-awesome.css}" />
    <link rel="stylesheet" type="text/css" th:href="@{/css/common/common.css}" />
    <link rel='stylesheet' type='text/css' th:href="@{https://fonts.googleapis.com/css?family=Open+Sans}" />
    <link rel='stylesheet' type='text/css' th:href="@{/css/common/form-list.css}" />
    <link rel='stylesheet' type='text/css' th:href="@{/css/customer/clientdetail.css}" />
</head>

<body>
    <!-- view -->
    <div id="wrapper">
        <!-- header -->
        <div class="header" th:include="./views/common/header :: headertpl"></div>
        <div id="page-wrapper">
            <!-- content -->
            <div class="content" id='app'>
                <!-- 页面位置 -->
                <div class="page-header-section">
                    <h3>客户管理</h3>
                </div>
                <div class="col-md-12" align='right'>
                    <button type='button' class='btn btn-primary btn-save' @click='saveFunc'>保存</button>
                </div>
                <!-- 表单 -->
                <div class="form-section form-inline">
                    <form class="js-form">
                        <div class="clearfix">
                            <div class='col-md-12 section-title'>
                                <label class="slide-btn js-slide-btn">
                                    <button type='button'>－</button>管理信息</label>
                            </div>
                            <div class="clearfix">
                                <div class='col-md-12'>
                                    <div class="form-group col-md-4">
                                        <label for="">主账号</label>
                                        <input type="text" class="form-control" v-model='customer_info.accountName' disabled>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">客户id</label>
                                        <input type="text" class='form-control' v-model='customer_info.id' disabled>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">客户来源</label>
                                        <input type="text" class='form-control' v-model='customer_info.source' disabled>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group col-md-4">
                                        <label for="">未登录天数</label>
                                        <input type="text" class='form-control' v-model='customer_info.loginDate' disabled>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">领用人</label>
                                        <input type="text" class='form-control' v-model='customer_info.receiveName' disabled>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">未回访天数</label>
                                        <input type="text" class='form-control' v-model='customer_info.returnDate' disabled>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="clearfix">
                            <div class='col-md-12 section-title'>
                                <label class="slide-btn js-slide-btn">
                                    <button type='button'>－</button>客户基本信息</label>
                            </div>
                            <div class="clearfix">
                                <div class='col-md-12'>
                                    <div class="form-group col-md-6">
                                        <label for="">公司名称</label>
                                        <input type="text" class="form-control" v-model='customer_info.companyName' name="" autofocus>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="">品牌名称</label>
                                        <input type="text" class='form-control' v-model='customer_info.bank'>
                                    </div>


                                </div>
                                <div class="col-md-12">
                                    <div class="form-group col-md-6">
                                        <label>城市</label>
                                        <select name="checks" id="" class="form-control" v-model='customer_info.province' @change="changeCitys($event,'province')">
                                            <option :value='item.id' v-for='item in province'>{{ item.name }}</option>
                                        </select>
                                        <select name="checks" id="" class="form-control" v-model='customer_info.city' @change="changeCitys($event,'citys')">
                                            <option :value="item.id" v-for='item in citys'>{{ item.name }}</option>
                                        </select>
                                        <select name="checks" id="" class="form-control" v-model='customer_info.district'>
                                            <option :value="item.id" v-for='item in districts'>{{ item.name }}</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label>地址</label>
                                        <input type="text" class="form-control address-area" v-model='customer_info.address'>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group col-md-6">
                                        <label>邮箱</label>
                                        <input type="text" class="form-control" v-model='customer_info.email'>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label>固定电话</label>
                                        <input type="text" class="form-control" v-model='customer_info.phone'>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="clearfix">
                            <div class='col-md-12 section-title'>
                                <label class="slide-btn js-slide-btn">
                                    <button type='button'>－</button>客户行业属性</label>
                            </div>
                            <div class="clearfix">
                                <div class='col-md-12'>
                                    <div class="form-group col-md-4">
                                        <label for="">企业类型</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择企业类型</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00009">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">行业类型</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择行业类型</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00003">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">贸易类型</label>
                                        <select name="checks" id="" class="form-control" v-model='customer_tag.tradeType'>
                                            <option value="">请选择</option>
                                            <option value="1">内销</option>
                                            <option value="2">外销</option>
                                            <option value="3">内外销</option>
                                        </select>
                                    </div>

                                </div>

                                <div class='col-md-12'>
                                    <div class="form-group col-md-4">
                                        <label for="">性别</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择性别</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00002">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">风格</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择风格</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00005">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">重点开发季节</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择重点开发季节</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00006">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class='col-md-12'>
                                    <div class="form-group col-md-4">
                                        <label for="">重点单品</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择重点单品</li>
                                        </ul>
                                        <checkbox-template :now_checked_data="feedback_list"></checkbox-template>
                                        <div class="filter-div js-filter-div" data-id="tag00004">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container">
                                                <!-- <checkbox-template :now_checked_data="feedback_list"></checkbox-template> -->
                                            </div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">材质</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择材质</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00007" data-type="1">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label for="">面料工艺</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择面料工艺</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00007" data-type="2">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                        <div class="clearfix">
                            <div class='col-md-12 section-title'>
                                <label class="slide-btn js-slide-btn">
                                    <button type='button'>－</button>客服信息</label>
                            </div>
                            <div class="clearfix">
                                <div class='col-md-12'>
                                    <div class="form-group col-md-6">
                                        <label for="">客户关注项</label>
                                        <ul class="form-control checked-list js-checked-list">
                                            <li>请选择客户关注项</li>
                                        </ul>
                                        <div class="filter-div js-filter-div" data-id="tag00014">
                                            <input type="text" placeholder="输入要搜索的文字" class='form-control js-search-trees'>
                                            <div class="checkbox-container js-checkbox-container"></div>
                                            <div class="btn-div">
                                                <button class="btn btn-default btn-sm js-cancel-checked" type="button">取消</button>
                                                <button class="btn btn-primary btn-sm js-add-checked" type="button">确定</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-6">
                                        <label for="">关注项备注</label>
                                        <input type="text" class='form-control' v-model='service_info.attentionRemark'>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group col-md-3">
                                        <label>服务阶段</label>
                                        <select name="" id="" class="form-control" v-model='service_info.serviceStage'>
                                            <option value="">请选择</option>
                                            <option value="tagval0000000282">新客期</option>
                                            <option value="tagval0000000283">服务期</option>
                                            <option value="tagval0000000284">流转期</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>客户情况</label>
                                        <select name="" id="" class="form-control" v-model='service_info.customerStatus'>
                                            <option value="">请选择</option>
                                            <option value="tagval0000000285">Z（正常）</option>
                                            <option value="tagval0000000286">Y（疑难客户）</option>
                                            <option value="tagval0000000287">L（长期不登录客户）</option>
                                            <option value="tagval0000000288">S（失联）</option>
                                            <option value="tagval0000000289">H（转行/倒闭）</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label>发票项</label>
                                        <select name="" id="" class="form-control" v-model='service_info.invoice'>
                                            <option value="">请选择</option>
                                            <option value="tagval0000000302">提前开票</option>
                                            <option value="tagval0000000303">暂缓开票</option>
                                            <option value="tagval0000000304">普通发票</option>
                                            <option value="tagval0000000305">专用发票</option>
                                            <option value="tagval0000000306">不需要发票</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-md-3">
                                        <label for="">网站权限</label>
                                        <input type="text" class='form-control' v-model='service_info.web' disabled>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class='col-md-1'>赠品内容</div>
                                    <div class='col-md-1'>快反应系列</div>

                                    <div class="col-md-10">
                                        <div class="col-md-12" v-for="item in service_info.complimentary || []">
                                            <div class='col-md-1' v-text="item.bookName"></div>
                                            <div class='col-md-4'>
                                                <label for="">最后一本到期事件</label>
                                                <input type="text" class="form-control" v-model="item.booKDate" disabled>
                                            </div>
                                            <div class='col-md-4'>
                                                <label for="">总数</label>
                                                <input type="text" class="form-control" v-model="item.count" disabled>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <!-- 列表 -->
                <div class="list-section">
                    <div class="clearfix">
                        <div class='col-md-12 section-title'>
                            <label class="slide-btn js-slide-btn">
                                <button type='button'>－</button>用户信息</label>
                        </div>
                        <div class="clearfix">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th width='1%'>
                                            <span>主</span>
                                        </th>
                                        <th>姓名</th>
                                        <th>角色</th>
                                        <th>职务</th>
                                        <th>状态</th>
                                        <th>移动电话</th>
                                        <th>工作电话</th>
                                        <th>微信号</th>
                                        <th>是否添加微信</th>
                                        <th>分机号</th>
                                        <th>QQ</th>
                                        <th>工作邮箱</th>
                                        <th>用户来源</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr align='center' v-for='item in contact_list'>
                                        <td>
                                            <input type="radio" :checked="item.main==1?true:false">
                                        </td>
                                        <td>
                                            <a :href="'/api/web/customer/adduser?customerid='+location_obj.id+'&contactid='+item.id" v-text="item.name"></a>
                                        </td>
                                        <td v-text="item.role"></td>
                                        <td v-text="item.duty"></td>
                                        <td v-text="item.status"></td>
                                        <td v-text="item.telephone"></td>
                                        <td v-text="item.akdkd"></td>
                                        <td v-text="item.weChat"></td>
                                        <td v-text="item.isweChat==1?'是':'否'"></td>
                                        <td v-text="item.extensionPhone"></td>
                                        <td v-text="item.qq"></td>
                                        <td v-text="item.email"></td>
                                        <td v-text="item.source"></td>
                                    </tr>
                                </tbody>
                            </table>
                            <a class="btn btn-primary add-user" href="/api/web/customer/adduser?type=1" title="新增用户">+新增用户</a>
                        </div>
                    </div>
                </div>
                <div class="list-section">
                    <div class="clearfix">
                        <div class='col-md-12 section-title'>
                            <label class="slide-btn js-slide-btn">
                                <button type='button'>－</button>账号信息</label>
                        </div>
                        <table class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>账号</th>
                                    <th>类型</th>
                                    <th>手机号</th>
                                    <th>未登录天数</th>
                                    <th>注册时间</th>
                                    <th>最后登录时间</th>
                                    <th>修改密码时间</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td v-text="account_info.accountName"></td>
                                    <td v-text="account_info.type"></td>
                                    <td v-text="account_info.phone"></td>
                                    <td v-text="account_info.loginDate"></td>
                                    <td v-text="account_info.register"></td>
                                    <td v-text="account_info.LastLoginDate!=undefined?account_info.LastLoginDate.substr(0,account_info.LastLoginDate.length-2):''"></td>
                                    <td v-text="account_info.UpdatePassDate!=undefined?account_info.UpdatePassDate.substr(0,account_info.UpdatePassDate.length-2):''"></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="list-section clearfix">
                    <div class="clearfix">
                        <div class='col-md-12 section-title'>
                            <label class="slide-btn js-slide-btn">
                                <button type='button'>－</button>回访记录</label>
                        </div>
                        <div class="clearfix">
                            <feedback-template :feedback_data="feedback_list"></feedback-template>
                            <div id="laypage-section" class='pull-right'></div>
                        </div>
                    </div>

                </div>
                <!-- tab切换 -->
                <div class="list-section">
                    <div class="clearfix">
                        <div class='col-md-12 section-title'>
                            <label class="slide-btn js-slide-btn">
                                <button type='button'>－</button>其他记录</label>
                        </div>
                        <div class="tab-section">
                            <ul class='tab-list js-tab-list'>
                                <li class='stats-active btn btn-default js-tab-item' data-key="receive">领</li>
                                <li class="btn btn-default js-tab-item" data-key="assist">协</li>
                                <li class="btn btn-default js-tab-item" data-key="activity">活</li>
                                <li class="btn btn-default js-tab-item" data-key="call">电</li>
                                <li class="btn btn-default js-tab-item" data-key="service">客</li>
                                <li class="btn btn-default js-tab-item" data-key="ground">地</li>
                                <li class="btn btn-default js-tab-item" data-key="order">订</li>
                                <li class="btn btn-default js-tab-item" data-key="invoice">发</li>
                                <li class="btn btn-default js-tab-item" data-key="open">开</li>
                                <li class="btn btn-default js-tab-item" data-key="shop">商</li>
                                <li class="btn btn-default">
                                    <a href="" title="">祥</a>
                                </li>
                                <li class="btn btn-default">
                                    <a href="" title="">统</a>
                                </li>
                            </ul>
                            <div class='stats-info js-stats-info'>
                                <div class='pr-quest' style="display:block;">
                                    <table class="table table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th>指派人</th>
                                                <th>状态</th>
                                                <th>员工</th>
                                                <th>开始时间</th>
                                                <th>结束时间</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr align='center' v-for='item in tab_list.receive'>
                                                <td v-text="item.designateName"></td>
                                                <td v-text="item.msgType==2?'领用':'指派'"></td>
                                                <td v-text="item.staffName"></td>
                                                <td v-text="item.startTime"></td>
                                                <td v-text="item.endTime"></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <div class='pr-assist'>
                                    <table class="table table-bordered table-hover">
                                        <thead>
                                            <tr>
                                                <th>操作者</th>
                                                <th>协助人</th>
                                                <th>第二协助人</th>
                                                <th>开始时间</th>
                                                <th>开始时间</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr align='center' v-for='item in tab_list.assist'>
                                                <td v-text="item.handlerName"></td>
                                                <td v-text="item.assistantName"></td>
                                                <td v-text="item.assistantName2"></td>
                                                <td v-text="item.startTime"></td>
                                                <td v-text="item.endTime"></td>
                                            </tr>
                                        </tbody>
                                    </table>
