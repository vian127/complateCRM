/*加载loading*/
.loading-div{z-index: 500;position: fixed;left: 0;top: 0px;width: 100%;height: 100%;background-color: rgba(245, 240, 227, 0.6);}
.loading{
    z-index: 10;position: absolute;left: 50%;top: 50%;width: 150px;height: 50px;margin-left: -75px;margin-top: -25px;
}
.loading span{
    display: inline-block;
    width: 15px;
    height: 100%;
    margin-right: 5px;
    background-color: #2196f3;
    animation: load 1.04s ease infinite;
}
.loading span:last-child{
    margin-right: 0px; 
}
@keyframes load{
    0%{
        opacity: 1;
        transform: scale(1.2);
    }
    100%{
        opacity: .2;
        transform: scale(.2);
    }
}
.loading span:nth-child(1){
    animation-delay:0.13s;
}
.loading span:nth-child(2){
    animation-delay:0.26s;
}
.loading span:nth-child(3){
    animation-delay:0.39s;
}
.loading span:nth-child(4){
    animation-delay:0.52s;
}
.loading span:nth-child(5){
    animation-delay:0.65s;
}



