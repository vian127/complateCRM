    public int getIZeroFixed() {
        return iZeroFixed;
    }

    public void setIZeroFixed(int zeroFixed) {
        iZeroFixed = zeroFixed;
        int iFixed = iZeroFixed;
        int iReturn = 0;
        while (iFixed > 1) {
            iReturn = iReturn + 1;
            iFixed = iFixed / 10;
        }
        this.iZeroNum = iReturn;
    }

